    "import math\n",
    "learning_base=0.0001\n",
    "decay_rate=1\n",
    "train_acc={}\n",
    "test_acc={}\n",
    "DNN_model.parameters=None\n",
    "for i in range(1,1001):\n",
    "    learning_rate=learning_base*math.pow(decay_rate,i)\n",
    "    DNN_model.set_params(learning_rate=learning_rate,iters=1)\n",
    "    print('iters:',i)\n",
    "    DNN_model.fit(data={'X':train_X_feature,'Y':train_Y_prob},init_param_seed=1)\n",
    "    print('compute acc')\n",
    "    train_y_label_pre=DNN_model.predict(X=train_X_feature)\n",
    "    test_y_label_pre=DNN_model.predict(X=test_X_feature)\n",
    "\n",
    "    accuracy=accuracy_score(y_true=train_y_label,\n",
    "                            y_pred=train_y_label_pre,\n",
    "                            normalize=True)\n",
    "    accuracy=np.round(accuracy,4)\n",
    "    train_acc[i]=accuracy\n",
    "    \n",
    "    accuracy=accuracy_score(y_true=test_y_label,\n",
    "                            y_pred=test_y_label_pre,\n",
    "                            normalize=True)\n",
    "    accuracy=np.round(accuracy,4)\n",
    "    \n",
